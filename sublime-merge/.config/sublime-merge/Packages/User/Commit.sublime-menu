[
    { "id": "end" },
    // The double-separator sets the user item visually apart.
    { "id": "modify", "caption": "-" },
    {
        "caption": "‚Ü∂ Reset --keep",
        "command": "git",
        "args": {"argv": ["reset", "--keep", "$commit"]},
    },
    {
        "caption": "Create \"fixup!\" Commit",
        "command": "git",
        "args": {"argv": ["commit", "--fixup", "$commit"]},
    },
    {
        "caption": "Rebase",
        "children": [
            {
                "caption": "Rebase --autosquash",
                "command": "git",
                "args": {"argv": ["rebase", "--autosquash", "$commit~1"]},
            },
            {
                "caption": "Rebase Onto‚Ä¶",
                "command": "show_command_palette",
                "args": {
                    "command": "git",
                    "args": {"argv": ["rebase", "$commit~1", "--onto", "$select_ref"]},
                },
            },
            {
                "caption": "Start Interactive Rebase‚Ä¶ (terminal)",
                "command": "git",
                "args": {"argv": ["rebase-terminal", "$commit~1"]},
            },
            {
                "caption": "Start Interactive Rebase‚Ä¶ (ST)",
                "command": "git",
                "args": {"argv": ["rebase", "-i", "$commit~1"]},
            },
        ],
    },
    { "id": "inspect", "caption": "-" },
    {
        "caption": "Copy Commit URL",
        "command": "git",
        "args": {"argv": ["copy-commit-url", "$commit"]},
    },
    {
        "caption": "üîç Analyze",
        "children": [
            {
                "caption": "Œ® Print Branches Containing $short_commit",
                "command": "git",
                "args": {"argv": ["branch", "-a", "--contains", "$commit"]}
            },
            {
               "caption": "‚òë Print Branches Merged in $short_commit",
               "command": "git", "args": {
                   "argv": ["branch", "--all", "--merged", "$commit"]
               }
            },
            {
                "caption": "‚àö Print Common Base Commit HEAD<->$short_commit",
                "command": "git",
                "args": {"argv": ["merge-base", "HEAD", "$commit"]}
            },
        ],
    },
    { "id": "user_end", "caption": "-", },
]
